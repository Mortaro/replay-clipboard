{
	"name": "replay-clipboard",
	"displayName": "Replay Clipboard",
	"description": "Pretends to type whatever you have in your clipboard",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:replay-clipboard.run"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "replay-clipboard.run",
				"title": "Replay Clipboard"
			}
		],
		"keybindings": [
			{
				"command": "replay-clipboard.run",
				"key": "ctrl+alt+r",
				"mac": "shift+cmd+r"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
